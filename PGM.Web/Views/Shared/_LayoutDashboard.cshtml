<body class="widescreen pace-done fixed-left-void dashboard-common">


    <div id="wrapper" class="forced enlarged main-dashboard">
        <!-- /.navbar-header -->
        <header id="site-header">
            <div class="navbar-header">
                <a class="navbar-brand" href="#">
                    <img class="navbar-logo" src="~/Content/Images/bepza-logo.png" alt="Logo">
                    <span class="navbar-branding">
                        <span class="brand-text">
                            Enterprise Resource Planning
                        </span>
                        <small class="brand-secondary-text">
                            Bangladesh Export Processing Zones Authority
                        </small>
                    </span>
                    <!-- /.navbar-branding -->
                </a>
                <!-- /.navbar-brand -->
            </div>


            <!-- Button mobile view to collapse sidebar menu -->
            <div class="navbar navbar-default" role="navigation">
                <div class="container">
                    <div class="navbar-collapse2">
                        <ul class="nav navbar-nav navbar-right top-navbar">
                            <li class="notifications">
                                <a href="#">
                                    <i class="glyphicon glyphicon-bell"></i>
                                    <span class="badge js-notification-count hide"></span>
                                </a>
                            </li>
                            <li class="dropdown iconify hide-phone">
                                <a href="#" onclick="javascript:toggle_fullscreen()"><i class="icon-resize-full-2"></i></a>
                            </li>
                            <li class="dropdown topbar-profile">
                                @Html.Partial("_LogOnDashboardPartial")
                            </li>

                        </ul>
                    </div>
                    <!--/.nav-collapse -->
                </div>
            </div>
            <div class="hide" id="divTmpNotificationMessages"></div>
        </header>

        <!-- Right Sidebar End -->
        <!-- Start right content -->
        <div class="content-page">
            <!-- ============================================================== -->
            <!-- Start Content here -->
            <!-- ============================================================== -->
            <div class="content">
                @RenderBody()

                <!-- Footer Start -->
                @*<footer>
                        TechnoVista &copy; 2016
                        <div class="footer-links pull-right">
                            <a href="#">About</a><a href="#">Support</a><a href="#">Terms of Service</a><a href="#">Legal</a><a href="#">Help</a><a href="#">Contact Us</a>
                        </div>
                    </footer>*@
                <!-- Footer End -->
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- End content here -->
        <!-- ============================================================== -->

    </div>

    <div class="md-overlay"></div>
    <!-- End of eoverlay modal -->
    <script>
        var resizefunc = [];
    </script>
