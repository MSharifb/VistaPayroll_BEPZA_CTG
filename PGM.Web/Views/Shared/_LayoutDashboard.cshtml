    <link rel="apple-touch-icon" href="@Url.Content("~/assets/img/apple-touch-icon.png")" />
    <link rel="apple-touch-icon" sizes="57x57" href="@Url.Content("~/assets/img/apple-touch-icon-57x57.png")" />
