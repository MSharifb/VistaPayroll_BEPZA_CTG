    <style>
        .validation-summary-errors {
            border-radius: 0;
            background-color: #f2dede;
            border-color: #ebccd1;
            color: #a94442;
        }
    </style>

    <script src="@Url.Content("~/assets/libs/jquery/jquery-1.11.1.min.js")"></script>
    
 
