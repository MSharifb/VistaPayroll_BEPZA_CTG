    <!-- PROJECT-SPECIFIC STYLES || TO OVERRIDE ALL THE PREVIOUS CSS DECLARATIONS -->
    <link href="@Url.Content("~/assets/css/project.css")" rel="stylesheet" type="text/css" />

    <link rel="shortcut icon" href="@Url.Content("~/Content/Images/bepza-logo.png")">

    <style>
        .validation-summary-errors {
            border-radius: 0;
            background-color: #f2dede;
            border-color: #ebccd1;
            color: #a94442;
        }
    </style>

    <script src="@Url.Content("~/assets/libs/jquery/jquery-1.11.1.min.js")"></script>

