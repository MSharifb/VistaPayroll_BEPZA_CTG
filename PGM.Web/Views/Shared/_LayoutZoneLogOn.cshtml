<body class="widescreen pace-done fixed-left-void dashboard-common">


    <div id="wrapper" class="forced enlarged main-dashboard">


        <!-- /.navbar-header -->
        <header id="site-header">
            <div class="navbar-header">
                <a class="navbar-brand" href="#">
                    <img class="navbar-logo" src="~/Content/Images/bepza-logo.png" alt="Logo">
                    <span class="navbar-branding">
                        <span class="brand-text">
                            Enterprise Resource Planning
                        </span>
                        <small class="brand-secondary-text">
                            Bangladesh Export Processing Zones Authority
                        </small>
                    </span>
                    <!-- /.navbar-branding -->
                </a>
                <!-- /.navbar-brand -->
            </div>


            <!-- Button mobile view to collapse sidebar menu -->
            <div class="navbar navbar-default" role="navigation">
                <div class="container">
                    <div class="navbar-collapse2">
                        <ul class="nav navbar-nav navbar-right top-navbar">

                            <li class="dropdown iconify hide-phone"><a href="#" onclick="javascript:toggle_fullscreen()"><i class="icon-resize-full-2"></i></a></li>
                            <li class="dropdown topbar-profile">
                                @*@if (Request.IsAuthenticated)
                                    {*@
                                <a href="javascript:void(0)" class="dropdown-toggle" data-toggle="dropdown">
                                    <span class="rounded-image topbar-profile-image">
                                        <img src="@Url.Content("~/assets/img/default-user.png")">
                                    </span> <strong> @User.Identity.Name</strong> <i class="fa fa-caret-down"></i>
                                </a>
                                <ul class="dropdown-menu">
                                    <li>
                                        <a class="dashboard-widget-link" href="@Url.Action("LogOff", "Account", new { area = "" })">
                                            <i class="icon-logout-1"></i> Log off
                                        </a>
                                    </li>
                                </ul>
                                @*}*@

                            </li>

                        </ul>
                    </div>
                    <!--/.nav-collapse -->
                </div>
            </div>
        </header>

        <!-- Right Sidebar End -->
        <!-- Start right content -->
        <div class="content-page">
            <!-- ============================================================== -->
            <!-- Start Content here -->
            <!-- ============================================================== -->
            <div class="content">
                @RenderBody()

                <!-- Footer Start -->
                @*<footer>
                        TechnoVista &copy; 2016
                        <div class="footer-links pull-right">
                            <a href="#">About</a><a href="#">Support</a><a href="#">Terms of Service</a><a href="#">Legal</a><a href="#">Help</a><a href="#">Contact Us</a>
                        </div>
                    </footer>*@
                <!-- Footer End -->
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- End content here -->
        <!-- ============================================================== -->

    </div>

    <div class="md-overlay"></div>
    <!-- End of eoverlay modal -->
    <script>
        var resizefunc = [];
    </script>
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->



    <script src="@Url.Content("~/assets/libs/bootstrap/js/bootstrap.min.js")"></script>

    <script src="@Url.Content("~/assets/libs/jqueryui/jquery-ui-1.10.4.custom.min.js")"></script>
    <script src="@Url.Content("~/assets/libs/jquery-ui-touch/jquery.ui.touch-punch.min.js")"></script>
