    <script src="@Url.Content("~/assets/libs/nifty-modal/js/classie.js")"></script>
    <script src="@Url.Content("~/assets/libs/nifty-modal/js/modalEffects.js")"></script>
    <script src="@Url.Content("~/assets/libs/sortable/sortable.min.js")"></script>
