    <script src="@Url.Content("~/assets/libs/bootstrap-select/bootstrap-select.min.js")"></script>
    <script src="@Url.Content("~/assets/libs/bootstrap-select2/select2.min.js")"></script>
    <script src="@Url.Content("~/assets/libs/magnific-popup/jquery.magnific-popup.min.js")"></script>
    <script src="@Url.Content("~/assets/libs/pace/pace.min.js")"></script>
    <script src="@Url.Content("~/assets/libs/bootstrap-datepicker/js/bootstrap-datepicker.js")"></script>
    <script src="@Url.Content("~/assets/libs/jquery-icheck/icheck.min.js")"></script>
    <!-- Demo Specific JS Libraries -->
    <script src="@Url.Content("~/assets/libs/prettify/prettify.js")"></script>

    <script src="@Url.Content("~/assets/js/init.js")"></script>

