@using PGM.Web.Areas.PGM.Models.BankAccount
@model IList<BankAccountModel>

<div class="frm-content">
    <div class="GroupBox search-content">
        <div id="jqSearch">
        </div>
    </div>
    <div class="GroupBox">
        <div class="button-crude button-left">
            @Ajax.ActionLink("Add New", "create", new AjaxOptions { OnComplete = "OnCompleteAddnew" })
        </div>
        <div class="clear"></div>
    </div>
    <div class="grid">
        <table id="jqGrid" cellpadding="0" cellspacing="0"></table>
        <div id="jqGridPager" style="text-align: center;">
        </div>
        @Html.Partial("_GridList", Model)
    </div>
</div>

