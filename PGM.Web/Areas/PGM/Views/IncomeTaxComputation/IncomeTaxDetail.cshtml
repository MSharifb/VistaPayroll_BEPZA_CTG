
@model PGM.Web.Areas.PGM.Models.IncomeTaxComputation.IncomeTaxComputationViewModel
@{
    ViewBag.Title = "IncomeTaxDetail";
    Layout = "~/Areas/PGM/Views/Shared/_Layout.cshtml";
}

@using (Ajax.BeginForm("GetDetailList", "IncomeTaxComputation", new AjaxOptions { HttpMethod = "POST" }, new { id = "frm" }))
{
     <fieldset>     
    <div class="GroupBox">
    <div id="message">
        @if (!string.IsNullOrEmpty(Model.Message))
        {
            <div class="success">
                @Model.Message
            </div>
        }
    </div>
    </div>
   
    <fieldset>
        <div id="jqSearch">
        
        </div>
    </fieldset> 
    <br />   
    <table id="jqGrid" cellpadding="0" cellspacing="0">
    </table>
    <div id="jqGridPager" style="text-align: center;">
    </div>  
    <div id="divList">
        @Html.Partial("_DetailGridList",Model)
    </div>   
    </fieldset>
}
<div class="button-crude button-left">
     @Html.ActionLink("Back to List", "Index")    
   
</div>
